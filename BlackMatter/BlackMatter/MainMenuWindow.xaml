<Window x:Class="BlackMatter.MainMenuWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BlackMatter"
        mc:Ignorable="d"
        Title="MainMenuWindow" Height="800" Width="800">
    <Grid >
        <Button Content="New Game" HorizontalAlignment="Left" Margin="427,265,0,0" VerticalAlignment="Top" Height="44" Width="263" Background="{x:Null}" Opacity="100" Click="NewGameClick" />
        <Button Content="Load Game" HorizontalAlignment="Left" Margin="427,348,0,0" VerticalAlignment="Top" Height="44" Width="263" Background="{x:Null}" Opacity="100" />
        <Button Content="High Score" HorizontalAlignment="Left" Margin="427,418,0,0" VerticalAlignment="Top" Height="44" Width="263" Background="{x:Null}" Opacity="100" Click="HighScoreWin" />
        <Button Content="Exit" HorizontalAlignment="Left" Margin="427,489,0,0" VerticalAlignment="Top" Height="44" Width="263" Background="{x:Null}" Opacity="100" Click="ExitClick" />
        

    </Grid>
</Window>
